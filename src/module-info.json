{
    "name": "Medal Monkey",
    "id": "aarontburn.Medal_Monkey",
    "version": "1.0.2",
    "author": "aarontburn",
    "author-id": "683193056e5989838dcf772c",
    "description": "'Embedding' external applications as a Nexus module. By monkeying it around.",
    "platforms": [],
    "link": "https://github.com/aarontburn/nexus-medal-monkey",
    "git-latest": {
        "git-username": "aarontburn",
        "git-repo-name": "nexus-medal-monkey"
    },
    "build": {
        "excluded": [
            "electron.ts"
        ],
        "build-version": 6,
        "included": [],
        "process": "./process/main.js",
        "replace": [
            {
                "from": "{EXPORTED_MODULE_ID}",
                "to": "%id%",
                "at": [
                    "./process/main.ts"
                ]
            },
            {
                "from": "{EXPORTED_MODULE_NAME}",
                "to": "%name%",
                "at": [
                    "./process/main.ts"
                ]
            }
        ]
    }
}